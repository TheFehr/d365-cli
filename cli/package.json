  "license": "MIT",
  "bugs": {
    "url": "https://github.com/derekfinlinson/d365-cli/issues"
  },
  "homepage": "https://github.com/derekfinlinson/d365-cli#readme",
  "dependencies": {
    "commander": "^2.19.0",
    "cross-spawn": "^6.0.5",
    "inquirer": "^6.2.0",
    "replace-in-file": "^3.4.2"
  },
  "devDependencies": {
    "commitizen": "^3.0.4",
    "cz-conventional-changelog": "^2.1.0",
    "jest": "^23.1.0",
